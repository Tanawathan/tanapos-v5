{
  "name": "tanapos-v5",
  "version": "5.0.0",
  "type": "module",
  "description": "TanaPOS V5 - AI-Enhanced POS System",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5173",
    "build": "npx tsc --noEmit && vite build",
    "build:debug": "npx tsc --noEmit --listFiles && vite build --logLevel info",
    "build:simple": "vite build --config vite.config.debug.ts",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "npx tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.0.0",
    "dotenv": "^17.2.1",
    "immer": "^10.1.1",
    "lucide-react": "^0.294.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.20.1",
    "recharts": "^3.1.0",
    "tailwind-merge": "^2.6.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.4"
  },
  "keywords": [
    "pos",
    "restaurant",
    "mobile",
    "react",
    "supabase",
    "typescript"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "author": "TanaPOS Development Team",
  "license": "MIT"
}
